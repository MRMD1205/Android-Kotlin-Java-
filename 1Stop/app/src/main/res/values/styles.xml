<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="VerifyUserCameraBrowseTextViewStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/colorGrayText</item>
        <item name="android:textSize">@dimen/_14ssp</item>
    </style>

    <style name="VerifyUserCameraBrowseImageViewStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_110sdp</item>
        <item name="android:layout_marginHorizontal">@dimen/_16sdp</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:background">@drawable/dotted_border</item>
        <item name="android:scaleType">centerCrop</item>
    </style>
</resources>